import java.util.ArrayList;


public class CompareSize implements CompareFiles{

	public CompareSize(){
		
	}
	
	public int compare(ArrayList<Object> File1, ArrayList<Object> File2){
		int size1 = (Integer) File1.get(1);
		int size2 = (Integer) File2.get(1);
		if (size1 < size2)
			return -1;
		else if (size1 > size2)
			return 2;
		return 0;
	}
}
